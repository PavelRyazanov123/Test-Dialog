<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graf"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.testnewdialog.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/home_fragment" >
        <action
            android:id="@+id/action_homeFragment_to_testFragment"
            app:destination="@id/testFragment" />
    </fragment>
    <dialog
        android:id="@+id/dialogFragment"
        android:name="com.example.testnewdialog.DialogFragment"
        android:label="dialog_fragment"
        tools:layout="@layout/dialog_fragment" />
    <dialog
        android:id="@+id/dialogColorPicker"
        android:name="com.example.testnewdialog.DialogColorPicker"
        android:label="dialog_color_picker"
        tools:layout="@layout/dialog_color_picker" />
    <fragment
        android:id="@+id/testFragment"
        android:name="com.example.testnewdialog.TestFragment"
        android:label="fragmetn_test"
        tools:layout="@layout/fragmetn_test" />
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.example.testnewdialog.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" />
    <dialog
        android:id="@+id/coordinateDialog"
        android:name="com.example.testnewdialog.CoordinateDialog"
        android:label="coordinate_dialog"
        tools:layout="@layout/coordinate_dialog" />
</navigation>